<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="Save Panel" ExtensionBundleVersion="1.1.0" Version="5.0">
    <ExtensionList>
        <Extension Id="se.lumens.SavePanel" Version="1.0"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PHSP" Version="15.0"/>
            <Host Name="PHXS" Version="15.0"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="5.0"/>
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="se.lumens.SavePanel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-speech-input</Parameter>
						<Parameter>--enable-media-stream</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Save Panel</Menu>
                    <Geometry>
                        <Size>
                            <Height>150</Height>
                            <Width>200</Width>
                        </Size>
                        <MaxSize>
                            <Height>300</Height>
                            <Width>400</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>100</Height>
                            <Width>100</Width>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icon.png</Icon>
                        <Icon Type="DarkNormal">./icon.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
